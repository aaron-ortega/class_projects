    "num_clusters = 25\n",
    "model = KMeans(n_clusters=num_clusters).fit(train)\n",
    "\n",
    "# get centers & cluster labels\n",
    "centers = model.cluster_centers_\n",
    "cluster_labels = model.labels_"
   ]
